   "source": [
    "import cv2\n",
    "\n",
    "img = cv2.imread('line.jpg')\n",
    "img_resize = cv2.resize(img, (640, 480))  \n",
    "edged = cv2.Canny(img_resize, 230, 250)\n",
    "cv2.imshow(\"Original Image\",img_resize)\n",
    "cv2.imshow(\"Edged Image\", edged)\n",
    "\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()"
   ]
